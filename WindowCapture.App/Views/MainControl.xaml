<UserControl
    x:Class="CapApp.Views.MainControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:prism="http://prismlibrary.com/"
    xmlns:viewmodels="clr-namespace:CapApp.ViewModels"
    d:DataContext="{d:DesignInstance Type=viewmodels:MainControlViewModel}"
    d:DesignHeight="300"
    d:DesignWidth="200"
    prism:ViewModelLocator.AutoWireViewModel="True"
    mc:Ignorable="d">
    <StackPanel Background="AliceBlue">
        <Button Command="{Binding AddCommand}" Content="ダミー画像" />
        <Button Command="{Binding ClearCommand}" Content="クリア" />
        <TextBlock Text="hoge" />
    </StackPanel>
</UserControl>
